library(ggplot2)
library(UsingR)
dat<- data.frame(Sexo=rep(c("Male", "Female"), each=10), Cortisol=c(rnorm(10, 10, 2),rnorm(10, 15, 2)))
boxplot(dat$Peso ~ dat$Sexo, xlab = "Sexo", ylab = "Peso")
dat<- data.frame(Sexo=rep(c("Male", "Female"), each=10), Cortisol=c(rnorm(10, 10, 2),rnorm(10, 15, 2)))
boxplot(dat$Cortisol ~ dat$Sexo, xlab = "Sexo", ylab = "Peso")
# t.test(Peso ~ Sexo, tilapia, alternative = c("two.sided"), var.equal=TRUE)
t.test(Cortisol ~ Sexo, dat, alternative = c("two.sided"),
var.equal=TRUE)
